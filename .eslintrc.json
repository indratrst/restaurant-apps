{
    "env": {
    "browser": true,
    "es2020": true,
    "node": true,
    "jasmine": true,
    "codeceptjs/codeceptjs": true
    },
    "extends": [
        "airbnb-base",
        "plugin:codeceptjs/recommended"
    ],
    "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
    },
    "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-alert": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "linebreak-style": 0,
    "no-tabs": ["error", { "allowIndentationTabs": true }],
    "max-len": ["error", { "ignoreTemplateLiterals": true }],
    "consistent-return": 0,
    "no-prototype-builtins": 0,
    "import/prefer-default-export": 0,
    "no-return-assign": 0
    },
    "plugins": [
        "jasmine", "codeceptjs"
    ]
}
